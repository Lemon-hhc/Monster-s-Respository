1.
#include<stdio.h>
int main()
{
    int count=0;
    int year = 0;
    for(year=1000;year<=2000;year++)
    {
        if(((year%4 ==0)&&(year%100!=0))||(year%400 ==0))//leap year computation
        {
            printf("%d ",year); 
            count++;
        }
    }
    printf("\ncount = %d \n",count);
}
2.
#inlcude<stdio.h>
#include<math.h>
int main()
{
    int j;
    int i=0;
    int count = 0;
    for(i=2;i<=200;i++)
    {
        int j=0;
        for(j=2;j<=sqrt(i);j++)
        {
        if(i%j == 0)
        {
            break;
        }
    }
    if(j>sqrt(i))
    {
        count++;
        printf("%d ",i);
    }
}
}
